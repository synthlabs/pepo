{
    "identifier": "desktop-capability",
    "platforms": [
        "macOS",
        "windows",
        "linux"
    ],
    "windows": [
        "main"
    ],
    "permissions": [
        "window-state:default",
        "opener:default",
        "store:default",
        "dialog:default",
        "dialog:allow-ask",
        "dialog:allow-message",
        "updater:default",
        "updater:allow-check",
        "updater:allow-download-and-install",
        "process:default",
        "process:allow-restart"
    ]
}