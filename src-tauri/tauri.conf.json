{
    "$schema": "https://schema.tauri.app/config/2",
    "app": {
        "security": {
            "csp": null
        },
        "windows": []
    },
    "build": {
        "beforeBuildCommand": "pnpm build",
        "beforeDevCommand": "pnpm dev --host",
        "devUrl": "http://localhost:1420",
        "frontendDist": "../build"
    },
    "bundle": {
        "active": true,
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "targets": [
            "deb",
            "rpm",
            "appimage",
            "nsis",
            "app",
            "dmg"
        ]
    },
    "identifier": "com.pepo.chat",
    "productName": "Pepo",
    "version": "0.2.0"
}